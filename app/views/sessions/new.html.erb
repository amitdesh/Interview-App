<h1>Interviewee Login</h1>

<% if flash[:error] %>
    <%= flash[:error] %>
<% end %>


<%= form_for :session, url: interviewee_login_path do |f| %>

    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :password %>
    <%= f.password_field :password %>
    
    <%= f.submit %>
<% end %>