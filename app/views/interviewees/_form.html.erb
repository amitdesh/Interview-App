<% if flash[:my_errors] %>
    <% flash[:my_errors].each do |err| %>
        Error Message: <%= err %><br>
    <% end %>
<% end %>


<%= form_for @interviewee do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :age %>
    <%= f.number_field :age %>

    <%= f.label :password %>
    <%= f.text_field :password %>
    
    <%= f.submit %>
<% end %>